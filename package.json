    "svelte": "^3.57.0",
    "vite": "^4.3.2"
  },
  "dependencies": {
    "@electron/rebuild": "^3.3.0",
    "@napi-rs/canvas": "^0.1.44",
    "@observablehq/plot": "^0.6.11",
    "arquero": "^5.2.0",
